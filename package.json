{
  "name": "Rage-Party",
  "version": "0.0.1",
  "description": "**Rage Party** is an online sandbox 2d third-person-shooter with coop and pvp possibilities. Players can co-op in endless mode of enemy waves or compete against each other.",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "npm run clean && webpack --config webpack.common.js && node index.js",
    "build": "webpack --config webpack.prod.js",
    "prod": "node index.js",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vermi4elli/Rage-Party.git"
  },
  "keywords": [
    "pixi.js",
    "webpack",
    "pixijs",
    "web",
    "game"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vermi4elli/Rage-Party/issues"
  },
  "homepage": "https://github.com/vermi4elli/Rage-Party#readme",
  "jest": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "./src/scripts/*.js",
      "./index.js"
    ],
    "coverageDirectory": "./coverage/"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-transform-modules-commonjs": "^7.12.1",
    "@babel/plugin-transform-runtime": "^7.12.1",
    "@babel/preset-env": "^7.12.1",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.1",
    "eslint": "^7.10.0",
    "html-webpack-plugin": "^4.5.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "jest": "^26.6.1",
    "rimraf": "^3.0.2",
    "webpack": "^5.3.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.2.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.12.1",
    "core-js": "^3.6.5",
    "fastify": "^3.5.1",
    "fastify-static": "^3.2.1",
    "pixi.js": "^5.3.3"
  }
}
